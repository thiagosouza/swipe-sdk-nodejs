{
  "name": "swipe-sdk-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "___start": "nodemon src/index.js localhost 8080",
    "__start": "nodemon",
    "start": "node --inspect-brk=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@swp/swipe-sdk": "0.9.2",
    "@types/node": "12.7.5",
    "eslint": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "express": "^4.17.1",
    "nodemon": "1.19.2",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  },
  "keywords": []
}
